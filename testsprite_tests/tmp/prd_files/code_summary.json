      "files": [
        "components/ui/background-slideshow.tsx",
        "components/sections/hero.tsx"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {
          "title": "Background Slideshow Feature",
          "description": "Frontend feature for displaying rotating photos",
          "version": "1.0.0"
