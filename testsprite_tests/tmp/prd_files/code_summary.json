        "info": {
          "title": "Gabon Events Calendar",
          "description": "Frontend calendar with Gabon holidays",
          "version": "1.0.0"
        }
      }
    },
    {
      "name": "Multi-Domain Routing",
      "description": "Middleware handling subdomain routing between main site (odillon.fr) and admin panel (admin.odillon.fr)",
      "files": [
        "middleware.ts"
      ],
      "api_doc": {
        "openapi": "3.0.0",
        "info": {
          "title": "Multi-Domain Routing",
          "description": "Middleware for subdomain-based routing",
          "version": "1.0.0"
        }
      }
    }
  ]
}
