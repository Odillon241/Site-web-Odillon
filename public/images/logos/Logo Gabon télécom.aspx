<div id="boxe" class="boxe notActiveBoxe">
    <form id="form2" name="form1">
        <div id="testDiv2">
            <div class="closeBoxe"><span>Assistance en ligne</span><a href="javascript:void(0);" onclick="closeBoxe();">X</a><a id="hideboxe" href="javascript:void(0);" onclick="toggleBoxe();"><img src="/Style Library/slimscroll/haut.png" /></a></div>
            <div class="chat-client"><span id="titreDiscussion"></span></div>
            <div id="messages">
                <!-- les messages du tchat -->
            </div>

            <div id="pseudoDiv" style="display: none;">
                <input type="text" name="idDiscussion" id="idDiscussion" value="0" /><br />
            </div>          
        </div>
        <div id="messageDiv" class="mess-ttre">
            <input type="text" name="message" id="message" class="mess-input" placeholder="Votre message ..." /> 
            <input type="submit" name="submit" value="Send" id="envoi" style="display: none" />
        </div>
           
    </form>
</div>
<script type="text/javascript">
    $('#testDiv2').slimscroll({});

    function setScroller(To) { 
        $('#testDiv2').slimScroll({ scrollBy: To + 'px' });
    }

    function closeBoxe() {        
        myStopFunction();
        $('#messages').html('');
        $('#boxe').hide();
    }

    function openBoxe() {
        $('#boxe').show();
        $('#boxe').removeClass("notActiveBoxe");
        $('#hideboxe').html('<img src="/Style Library/slimscroll/bas.png" />');
    }

    function toggleBoxe() {
        if (!$("#boxe").hasClass("notActiveBoxe")) {
            $('#boxe').addClass("notActiveBoxe");
            $('#hideboxe').html('<img src="/Style Library/slimscroll/haut.png" />');
        }
        else {
                        
            $('#boxe').removeClass("notActiveBoxe");
            $('#hideboxe').html('<img src="/Style Library/slimscroll/bas.png" />');
        }
    }

    $('#envoi').click(function (e) {
        e.preventDefault(); // on empêche le bouton d'envoyer le formulaire
        traiter();
    });

    function traiter() {
        var idDiscussion = encodeURIComponent($('#idDiscussion').val()); // on sécurise les données
        var message = encodeURIComponent($('#message').val());

        if (idDiscussion != "" && message != "") { // on vérifie que les variables ne sont pas vides
            $.ajax({
                url: "https://www.moov-africa.ga/particulier/Pages/Chater/ChatTraitement.aspx", // on donne l'URL du fichier de traitement
                type: "POST", // la requête est de type POST
                data: "action=traitement&idDiscussion=" + idDiscussion + "&message=" + message + "&connected=1&sending=1", // et on envoie nos données
                cache: false,
                success: function (result) {
                    $('#idDiscussion').val(result.split("<")[0]);
                    //$('#titreDiscussion').html("Discussion: " + $('#idDiscussion').val());
                    $('#message').val("");
                    myStopFunction();                   
                    getMessages(0);
                    setScroller(100000);
                }
            });
        }
    }

    function getCookies() {   
            $.ajax({
                url: "https://www.moov-africa.ga/particulier/Pages/Chater/ChatTraitement.aspx", // on donne l'URL du fichier de traitement
                type: "POST", // la requête est de type POST
                data: "action=cookie", // et on envoie nos données
                cache: false,
                success: function (result) {
                    $('#idDiscussion').val(result.split("<")[0]);
                    //$('#titreDiscussion').html("Discussion: " + $('#idDiscussion').val());
                    $('#message').val("");                    
                    getMessages(0);
                    setScroller(100000);
                }
            });

    }
    getCookies();

    var myVar;
    function myStopFunction() {
        clearTimeout(myVar);
    }

    function getMessages(timer) {
        idDiscussion = encodeURIComponent($('#idDiscussion').val()); // on sécurise les données  

        myVar = setTimeout(function () {
            $.ajax({
                url: "https://www.moov-africa.ga/particulier/Pages/Chater/ChatTraitement.aspx", // on passe le idDiscussion du client
                type: "POST", // la requête est de type POST
                data: "action=charger&idDiscussion=" + idDiscussion, // et on envoie nos données
                success: function (html) {
                    $('#messages').html(html.split("<!")[0]);
                    if (timer == 0) setScroller(100000);
                }
            });            
            getMessages(3000);
        }, timer);
    }

    $(window).bind("beforeunload", function () {
        desconnect();
    })

    function desconnect() {
        var idDiscussion = encodeURIComponent($('#idDiscussion').val()); // on sécurise les données
        $.ajax({
            url: "https://www.moov-africa.ga/particulier/Pages/Chater/ChatTraitement.aspx", // on donne l'URL du fichier de traitement
            type: "POST", // la requête est de type POST
            data: "action=traitement&idDiscussion=" + idDiscussion + "&message=&connected=0&sending=0", // et on envoie nos données
            cache: false,
            success: function (result) {
                return false;
            }
        });
        return false;
    }

    function playSound() {
        var audioElement = document.createElement('audio');
        audioElement.setAttribute('src', '/Style Library/slimscroll/Beep.mp3');

        audioElement.addEventListener('load', function () {
            this.play();
        }, false);
    }
</script> 
         
